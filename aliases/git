alias git="noglob git"
alias gc="git checkout"
alias gcm="git checkout master"
alias gc-="git checkout -"

alias ga.="git add ."

alias gco="git commit"
alias gcom="git commit -m"

alias gp="git push"

alias gl="git pull"
alias glr="git pull --rebase"

alias gs="git status"
alias gst="git status"

alias gm="git merge"

alias gr="git rebase"

alias gb="git branch"

alias gd="git diff"

alias glrp="glr && gp"
